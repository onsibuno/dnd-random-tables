{% extends 'base.html.twig' %}

{% block title %}Hello NewTableController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">

		{{form(form)}}
		{# {{ form_start(form) }}
		{{ form_row(form.TableName) }}
        {% set content = form_row(form.Contents) %}
		<ul class="tags">
            {% for i in 0..10 %}
            <li>{{ content|raw }}</li>
                
            {% endfor %}
		</ul>

		{{ form_end(form) }} #}
	</div>
{% endblock %}
